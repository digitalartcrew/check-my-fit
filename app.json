{
  "expo": {
    "name": "Check My Fit",
    "slug": "check-my-fit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f0f"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.checkmyfit.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Check My Fit needs camera access to photograph your outfits.",
        "NSPhotoLibraryUsageDescription": "Check My Fit needs photo library access to upload outfit photos.",
        "NSPhotoLibraryAddUsageDescription": "Check My Fit needs permission to save photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f0f0f"
      },
      "package": "com.checkmyfit.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-camera"
    ]
  }
}
